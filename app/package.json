{
  "name": "@fastify-app-tony/app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "clean": "yarn rimraf ./dist && yarn rimraf ./node_modules",
    "codegen": "graphql-codegen",
    "dev": "NODE_ENV=development node --loader extensionless --experimental-strip-types --watch-path=./src ./src/index.ts | yarn pino-pretty --singleLine",
    "start": "NODE_ENV=production node --loader extensionless --experimental-strip-types ./src/index.ts",
    "test": "vitest run ./src -c ./vite.config.test.mjs",
    "test:watch": "vitest run ./src -c ./vite.config.test.mjs --watch"
  },
  "dependencies": {
    "@envelop/core": "^5.2.3",
    "@envelop/graphql-modules": "^7.1.3",
    "@fastify/autoload": "^6.2.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/middie": "^9.0.3",
    "@fastify/static": "^8.1.1",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/schema": "^10.0.23",
    "@mcansh/remix-fastify": "^4.0.8",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-router/node": "^7.5.0",
    "@sinclair/typebox": "^0.34.33",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/recipes": "^0.5.5",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^9.1.0",
    "eventive": "^3.0.0",
    "extensionless": "^1.9.9",
    "fastify": "^5.2.2",
    "fastify-graceful-shutdown": "^4.0.1",
    "fastify-plugin": "^5.0.1",
    "glob": "^11.0.1",
    "graphql": "^16.10.0",
    "graphql-modules": "patch:graphql-modules@npm%3A3.0.0#~/.yarn/patches/graphql-modules-npm-3.0.0-27f60c3f79.patch",
    "graphql-sock": "^1.0.1",
    "graphql-yoga": "^5.13.2",
    "isbot": "^5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-relay": "^18.2.0",
    "react-router": "^7.5.0",
    "react-router-relay": "workspace:^",
    "relay-runtime": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/graphql-modules-preset": "^4.0.16",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-resolvers": "^4.5.0",
    "@react-router/dev": "^7.5.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@types/react-relay": "^18.2.0",
    "@types/relay-runtime": "^18.2.5",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "graphql-codegen-typescript-resolvers-define": "^0.3.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "relay-compiler": "^18.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.5",
    "vite-plugin-cjs-interop": "^2.2.0",
    "vite-plugin-relay-lite": "^0.10.0",
    "vitest": "^3.1.1"
  },
  "extensionless": {
    "lookFor": [
      "js",
      "mjs",
      "cjs",
      "ts",
      "tsx"
    ]
  },
  "ultra": {
    "concurrent": [
      "clean",
      "build"
    ]
  }
}
